<%= simple_form_for(@reading) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :week %>
    <%= f.input :title %>
    <%= f.input :intro, :as => :ckeditor, :input_html => { :ckeditor => {:toolbar => 'Full'} } %>
    <%= f.input :url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
