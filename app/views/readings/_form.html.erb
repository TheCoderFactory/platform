<%= simple_form_for(@reading) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :week %>
    <%= f.input :title %>
    <%= f.input :intro, :input_html => { :class => 'form-control', rows: 10 } %>
    <%= f.input :url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
