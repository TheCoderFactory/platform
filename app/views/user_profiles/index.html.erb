<h1>Listing user_profiles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Picture</th>
      <th>Background</th>
      <th>Hobbies</th>
      <th>Facebook</th>
      <th>Linkedin</th>
      <th>Twitter</th>
      <th>Website</th>
      <th>Nickname</th>
      <th>Slug</th>
      <th>Github</th>
      <th>Phone</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @user_profiles.each do |user_profile| %>
      <tr>
        <td><%= user_profile.user %></td>
        <td><%= user_profile.picture %></td>
        <td><%= user_profile.background %></td>
        <td><%= user_profile.hobbies %></td>
        <td><%= user_profile.facebook %></td>
        <td><%= user_profile.linkedin %></td>
        <td><%= user_profile.twitter %></td>
        <td><%= user_profile.website %></td>
        <td><%= user_profile.nickname %></td>
        <td><%= user_profile.slug %></td>
        <td><%= user_profile.github %></td>
        <td><%= user_profile.phone %></td>
        <td><%= link_to 'Show', user_profile %></td>
        <td><%= link_to 'Edit', edit_user_profile_path(user_profile) %></td>
        <td><%= link_to 'Destroy', user_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User profile', new_user_profile_path %>
