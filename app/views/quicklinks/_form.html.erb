<%= simple_form_for(@quicklink) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :icon %>
    <%= f.input :name %>
    <%= f.input :url %>
    <%= f.input :active %>
    <%= f.input :important %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
