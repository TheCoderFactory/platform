<h1>Listing quicklinks</h1>

<table>
  <thead>
    <tr>
      <th>Icon</th>
      <th>Name</th>
      <th>Url</th>
      <th>Active</th>
      <th>Important</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @quicklinks.each do |quicklink| %>
      <tr>
        <td><%= quicklink.icon %></td>
        <td><%= quicklink.name %></td>
        <td><%= quicklink.url %></td>
        <td><%= quicklink.active %></td>
        <td><%= quicklink.important %></td>
        <td><%= link_to 'Show', quicklink %></td>
        <td><%= link_to 'Edit', edit_quicklink_path(quicklink) %></td>
        <td><%= link_to 'Destroy', quicklink, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quicklink', new_quicklink_path %>
